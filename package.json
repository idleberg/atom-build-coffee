{
  "name": "build-coffeescript",
  "version": "0.1.0",
  "description": "Atom Build provider for CoffeeScript, compiles CoffeeScript",
  "repository": "https://github.com/idleberg/atom-build-coffeescript",
  "license": "MIT",
  "keywords": [
    "build",
    "compile",
    "coffeescript",
    "javascript",
    "coffee"
  ],
